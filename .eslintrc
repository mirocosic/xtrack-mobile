{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true
  },
  "extends": "airbnb",
  "rules": {
      "semi": 0,
      "quotes": ["warn", "double"],
      "padded-blocks": ["off", "always"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-boolean-value": ["always"],
      "react/jsx-closing-bracket-location": [<disabled>]
    }
}
